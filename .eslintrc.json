{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["react-hooks", "unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports": "warn",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": [
      "warn",
      {
        "additionalHooks": ""
      }
    ]
  }
}
